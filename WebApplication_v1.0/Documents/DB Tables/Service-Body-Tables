                                               Service_All
                Service_Name           |           Customer_Name     |       Time
            
Query =  Create table Service_All(Request_Number varchar(50) primary key,Service_Name varchar(50) not null,Customer_Name varchar(100) not null,Time varchar(100) not null,Status int not null);      

