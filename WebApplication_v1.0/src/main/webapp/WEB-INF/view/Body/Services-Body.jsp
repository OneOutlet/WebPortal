<%@ taglib uri="http://www.springframework.org/tags" prefix="spring"%>
<div class="row">

	<!--  Card deck -->
	<div class="card-deck">

		<!-- Electrician Card -->
		<div class="card mb-4 xs-4">

			<!--Card image-->
			<div class="view overlay">
				<img class="card-img-top"
					src="<spring:url value="/Edit/EditHome/Service-Body/electrician.jpg"/>"
					alt="Card image cap"> <a href="#!">
					<div class="mask rgba-white-slight"></div>
				</a>
			</div>

			<!--Card content-->
			<div class="card-body">

				<!--Title-->
				<h4 class="card-title">Electrician</h4>
				<!--Text-->
				<p class="card-text"></p>
				<!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
				<button type="button" data-target="#modalElectricianForm"
					data-toggle="modal" class="btn btn-light-blue btn-md blue-gradient">BOOK
					NOW</button>

			</div>

		</div>
		<!-- Card -->

		<!--Plumber Card -->
